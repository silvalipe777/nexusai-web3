    "test": "npx hardhat test",
    "serve": "python -m http.server 8080"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.28.4"
  },
  "keywords": [
    "web3",
    "nft",
    "ai",
    "agents",
    "ethereum",
    "solidity"
